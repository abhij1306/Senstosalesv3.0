{
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "printWidth": 100,
  "trailingComma": "es5",
  "arrowParens": "always",
  "endOfLine": "auto",
  "overrides": [
    {
      "files": "*.tsx",
      "options": {
        "parser": "typescript",
        "printWidth": 100
      }
    },
    {
      "files": "*.css",
      "options": {
        "parser": "css",
        "printWidth": 120
      }
    }
  ]
}
